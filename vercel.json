{
  "version": 2,
  "buildCommand": "pnpm -w run vercel-build",
  "installCommand": "corepack enable && corepack prepare pnpm@10.14.0 --activate && pnpm install --frozen-lockfile",
  "outputDirectory": "packages/client/dist",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
